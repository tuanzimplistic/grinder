Q_REQUIRE_ID(300, (sig != 0)
    &amp;&amp; (tickRate &lt; QF_MAX_TICK_RATE));

me-&gt;next      = (QTimeEvt *)0;
me-&gt;ctr       = 0U;
me-&gt;interval  = 0U;
