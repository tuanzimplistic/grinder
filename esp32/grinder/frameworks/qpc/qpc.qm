*/</documentation>
    <!--${QF::QActive::ctor::initial}-->
    <parameter name="initial" type="QStateHandler const"/>
    <code>static QActiveVtable const vtable = { /* QActive virtual table */
    { &amp;QHsm_init_,
      &amp;QHsm_dispatch_
#ifdef Q_SPY
      ,&amp;QHsm_getStateHandler_
#endif
    },
    &amp;QActive_start_,
    &amp;QActive_post_,
    &amp;QActive_postLIFO_
