   <!--${QF-types::QF_LOG2::x}-->
   <parameter name="x" type="QPSetBits"/>
   <code>static uint8_t const log2LUT[16] = {
    0U, 1U, 2U, 2U, 3U, 3U, 3U, 3U,
    4U, 4U, 4U, 4U, 4U, 4U, 4U, 4U
